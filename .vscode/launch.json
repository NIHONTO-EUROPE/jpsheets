    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current Files",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "kana.js",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "debug"
            ],
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Kana Cairo",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/kana-cairo.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Kana QPainter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/kana-qpainter.py",
            "console": "integratedTerminal",
        }
    ],
}
